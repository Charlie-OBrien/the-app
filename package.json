{
  "name": "TheApp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle-ios": "react-native bundle --entry-file index.js --platform ios --bundle-output ios/main.jsbundle",
    "build-ios": "npm run bundle-ios && xcodebuild  -project ios/TheApp.xcodeproj -scheme TheApp -sdk iphonesimulator -destination platform='iOS Simulator',name='iPhone 7' -derivedDataPath ios/build",
    "build-android": "cd android && ./gradlew assembleDebug",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.53.2",
    "react-native-elements": "^1.0.0-beta2",
    "react-native-navigation": "^1.1.382",
    "react-native-vector-icons": "^4.5.0"
  },
  "devDependencies": {
    "babel-jest": "22.2.2",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.3.0",
    "metro-bundler": "^0.22.1",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
